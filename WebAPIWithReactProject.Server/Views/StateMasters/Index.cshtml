@model IEnumerable<WebAPIWithReactProject.Server.Models.StateMaster>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Statename)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Details)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Flag)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Phno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Emailid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Finstartyear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Finendyear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Usedflag)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Logo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Statename)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Details)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Flag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Emailid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Finstartyear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Finendyear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usedflag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Logo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Srno">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Srno">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Srno">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
